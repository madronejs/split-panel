{
  "name": "@madronejs/splitpanel",
  "version": "0.0.7",
  "private": false,
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.css",
    "types/**/*.d.ts"
  ],
  "keywords": [
    "splitpanel",
    "splitpane",
    "panel",
    "drag",
    "draggable",
    "resizable",
    "madrone",
    "vue",
    "web components"
  ],
  "types": "types/core/index.d.ts",
  "typesVersions": {
    "*": {
      "vue3": [
        "./types/render/vue3.d.ts"
      ],
      "webc": [
        "./types/render/webComponent.d.ts"
      ],
      "animate": [
        "./types/plugins/animate.d.ts"
      ],
      "draggable": [
        "./types/plugins/draggable.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./vue3": {
      "import": "./dist/vue3.mjs",
      "require": "./dist/vue3.js"
    },
    "./webc": {
      "import": "./dist/webComponent.mjs",
      "require": "./dist/webComponent.js"
    },
    "./animate": {
      "import": "./dist/animate.mjs",
      "require": "./dist/animate.js"
    },
    "./draggable": {
      "import": "./dist/draggable.mjs",
      "require": "./dist/draggable.js"
    },
    "./style": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "build-types": "rm -rf types/* && tsc -p ./tsconfig.json && tsc-alias -p tsconfig.json",
    "build": "rm -rf dist/* && pnpm build-types && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/animejs": "^3.1.8",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unicorn": "^48.0.1",
    "sass": "^1.68.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2",
    "vite": "^4.4.10"
  },
  "dependencies": {
    "animejs": "^3.2.1",
    "drag-drop-touch": "^1.3.1",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "@madronejs/core": "^1.0.16",
    "vue": "^3.3.4"
  },
  "engines": {
    "node": "^18.8",
    "pnpm": "^8.7.0"
  },
  "packageManager": "pnpm@8.7.0"
}
